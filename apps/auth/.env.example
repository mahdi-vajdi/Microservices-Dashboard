# These secrets will be used to decode jwt tokens.
# Must use different ones for access and refresh to prevent security issues
JWT_ACCESS_SECRET=a.long.unique.random.string
JWT_REFRESH_SECRET=a.long.unique.random.string

# URI for nats. in this case its docker compose uri
NATS_URI=nats://nats:4222

# The urls for grpc clients
AUTH_GRPC_URL=auth:5001
ACCOUNT_GRPC_URL=account:5001
AGENT_GRPC_URL=agent:5001

